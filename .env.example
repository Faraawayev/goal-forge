# Example environment variables for local development
# Copy this file to .env and replace placeholders with real values.

# Postgres connection (example for local docker-compose DB)
DATABASE_URL=postgres://postgres:postgres@localhost:5432/goalforge

# App base URL (used by CI / deployment scripts when Render is not used)
BASE_URL=http://localhost:5000

# Dev seed token (optional) - set to a secure random value for protected seed endpoint
DEV_SEED_TOKEN=dev-token-placeholder

# Optional deploy/CI secrets (leave empty in example)
RENDER_API_KEY=
RENDER_SERVICE_ID=
VERCEL_TOKEN=
VERCEL_PROJECT_ID=
VERCEL_ORG_ID=
